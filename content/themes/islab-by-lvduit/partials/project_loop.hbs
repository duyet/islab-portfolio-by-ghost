{{! This is the post loop - each post will be output using this markup }}

<script type="text/javascript">
    var project_data = [];

{{#foreach posts}}
{{#has tag="project"}}

project_data.push({
  'title' : '{{title}}',
  'description' : '{{excerpt}}',
  'thumbnail' : ['{{image}}'],
  'large' : ['{{image}}'],
  'button_list' : [
      { 'title':'Details', 'url' : '{{url}}' },
  ],
  'tags'  : ['{{tags}}']
});

{{/has}}
{{/foreach}} 
</script>